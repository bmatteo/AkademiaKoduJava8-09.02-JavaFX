<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<FlowPane xmlns="http://javafx.com/javafx"
          xmlns:fx="http://javafx.com/fxml"
          fx:controller="pl.academy.code.controllers.Pane2Controller">
    <children>
        <Button text="Click me !!!!" fx:id="button" onAction="#action"
                prefHeight="60" prefWidth="250"/>

        <TextField fx:id="textField" prefWidth="250"/>

        <Label text="Jestem Labelka !!" fx:id="label"
               prefHeight="60" prefWidth="250"/>

        <Button text="Go back" fx:id="goBackButton" onAction="#goBack"
                prefHeight="60" prefWidth="250"/>
    </children>
</FlowPane>
